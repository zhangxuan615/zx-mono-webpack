{
  "name": "mini-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "yarn dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./configs/webpack.config.babel.js",
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.babel.js",
    "build:ana": "cross-env BUNDLE_ANNALYZE=analyze npm run build",
    "dev:cjs": "webpack serve --config ./configs/webpack.dev.config.js",
    "build:cjs": "webpack  --config ./configs/webpack.prod.config.js",
    "prepare": "husky install"
  },
