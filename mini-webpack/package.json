  "dependencies": {},
  "devDependencies": {
    "loader-utils": "^2.0.0",
    "schema-utils": "^3.1.1",
    "tapable": "^2.2.0"
  },
  "workspaces": [
    "mini-webpack",
    "webpack-demo/*"
  ]
}
